<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="parentPane" prefHeight="254.0" prefWidth="495.0" style="-fx-background-color: #554F53;" stylesheets="@main.css" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.kevinpita.comicstore.view.ConfigurationController">
    <children>
        <Label layoutX="11.0" layoutY="60.0" prefHeight="20.0" prefWidth="100.0" text="%server" textFill="WHITE">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label layoutX="11.0" layoutY="132.0" prefHeight="20.0" prefWidth="100.0" text="%password" textFill="WHITE">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <TextField fx:id="configServer" layoutX="117.0" layoutY="54.0" prefHeight="30.0" prefWidth="200.0" promptText="%server">
            <font>
                <Font size="16.0" />
            </font>
            <styleClass>
                <String fx:value="configStyle" />
                <String fx:value="configTextField" />
            </styleClass>
        </TextField>
        <TextField fx:id="configPassword" layoutX="117.0" layoutY="126.0" prefHeight="30.0" prefWidth="200.0" promptText="%password">
            <font>
                <Font size="16.0" />
            </font>
            <styleClass>
                <String fx:value="configStyle" />
                <String fx:value="configTextField" />
            </styleClass>
        </TextField>
        <Button fx:id="configCheck" layoutX="334.0" layoutY="88.0" mnemonicParsing="false" onAction="#checkConfig" prefHeight="30.0" prefWidth="150.0" text="%check">
            <font>
                <Font size="18.0" />
            </font>
            <styleClass>
                <String fx:value="configStyle" />
                <String fx:value="configHover" />
            </styleClass>
        </Button>
        <Button fx:id="configSave" layoutX="213.0" layoutY="193.0" mnemonicParsing="false" onAction="#saveConfigEdit" prefHeight="30.0" prefWidth="120.0" text="%save">
            <font>
                <Font size="18.0" />
            </font>
            <styleClass>
                <String fx:value="configStyle" />
                <String fx:value="configHover" />
            </styleClass>
        </Button>
        <Button fx:id="configCancel" layoutX="364.0" layoutY="193.0" mnemonicParsing="false" onAction="#cancelConfigEdit" prefHeight="30.0" prefWidth="120.0" text="%cancel">
            <font>
                <Font size="18.0" />
            </font>
            <styleClass>
                <String fx:value="configStyle" />
                <String fx:value="configHover" />
            </styleClass>
        </Button>
    </children>
</AnchorPane>
